/**
 * Definition for a binary tree node.
 * public class TreeNode {
 *     int val;
 *     TreeNode left;
 *     TreeNode right;
 *     TreeNode() {}
 *     TreeNode(int val) { this.val = val; }
 *     TreeNode(int val, TreeNode left, TreeNode right) {
 *         this.val = val;
 *         this.left = left;
 *         this.right = right;
 *     }
 * }
 */
class Solution {
    int n[]=new int[10000];
    int i=0;
    public boolean findTarget(TreeNode root, int k) {
        dfs(root);
       int f=0;
       int l=i-1;
        while(f<l){
          int sum=n[f]+n[l];
            if(sum==k)
            return true;
            else if(sum<k)
            f++;
            else
            l--;
        }
        return false;

        }
        private void dfs(TreeNode root){
            if(root==null)
            return;
            dfs(root.left);
            n[i++]=root.val;
            dfs(root.right);


        }


        
    }
