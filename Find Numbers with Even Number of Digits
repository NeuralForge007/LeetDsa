class Solution {
    public int findNumbers(int[] nums) {
        int s=0;
        for(int i:nums){
            int c=0;
            if(i==0)
            c++;
            while(i>=1){
                c++;
                i=i/10;
            }
            if(c%2==0)
            s++;


        }
        return s;

        
    }
}
